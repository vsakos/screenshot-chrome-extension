{
  "name": "Screenshot Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Screenshot Extension",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["<all_urls>", "activeTab", "tabs", "storage"]
}
